{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "functions": {
    "dist/src/main.js": {
      "includeFiles": [
        "node_modules/@scalar/**/*",
        "node_modules/@scalar/core/**/*"
      ]
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/dist/src/main.js"
    }
  ]
}
