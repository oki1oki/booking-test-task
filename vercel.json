{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "functions": {
    "dist/src/main.js": {
      "maxDuration": 30,
      "includeFiles": ["node_modules/@scalar/**/*"]
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/dist/src/main.js"
    }
  ]
}
